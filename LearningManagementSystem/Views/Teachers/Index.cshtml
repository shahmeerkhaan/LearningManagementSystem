@model IEnumerable<LearningManagementSystem.Models.Teacher>

@{
    ViewBag.Title = "Index";
}

<style>
    body {
        background-color: #091b1a;
        font-family: "Roboto", sans-serif;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }
    
</style>
<body>
    <table class="table">
        <tr>
            <th style="color:white; font-size:20px">
                @Html.DisplayNameFor(model => model.TName)
            </th>
            <th style="color:white; font-size: 20px">
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th style="color:white; font-size: 20px">
                @Html.DisplayNameFor(model => model.TPassword)
            </th>
            <th style="color:white; font-size: 20px">
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th style="color:white; font-size: 20px">
                @Html.DisplayNameFor(model => model.gender)
            </th>
            <th style="color:white; font-size: 20px">
                @Html.DisplayNameFor(model => model.Taddress)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.TName)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.TPassword)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.Age)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.gender)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.Taddress)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Tid }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Tid })
                </td>
            </tr>
        }

    </table>
    <a>@Html.ActionLink("Back To Main Page", "Index", "Admin")</a>
</body>

