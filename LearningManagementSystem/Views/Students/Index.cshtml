@model IEnumerable<LearningManagementSystem.Models.Student>

@{
    ViewBag.Title = "Index";
}
<style>
    body {
        background-color: #091b1a;
        font-family: "Roboto", sans-serif;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }
</style>
<body>
    <table class="table">
        <tr>
            <th style="color:black; font-size:20px">
                @Html.DisplayNameFor(model => model.StdName)
            </th>
            <th style="color:black; font-size:20px">
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th style="color:black; font-size:20px">
                @Html.DisplayNameFor(model => model.SPassword)
            </th>
            <th style="color:black; font-size:20px">
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th style="color:black; font-size:20px">
                @Html.DisplayNameFor(model => model.gender)
            </th>
            <th style="color:black; font-size:20px">
                @Html.DisplayNameFor(model => model.Saddress)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.StdName)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.SPassword)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.Age)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.gender)
                </td>
                <td style="color:white; font-size:15px">
                    @Html.DisplayFor(modelItem => item.Saddress)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.stdid }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.stdid })
                </td>
            </tr>
        }

    </table>
    <a>@Html.ActionLink("Back To Main Page", "Index", "Admin")</a>
</body>

